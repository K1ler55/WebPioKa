@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    ViewBag.Title = "Change";
}
<div class="container">
<h2>Change</h2>
<h2>@ViewBag.Pos</h2>

@{ 
    
    foreach(FlowModel<string> f in ViewBag.Model.list)
    {
        <h2>@f.name = @f.value</h2>        
        <h2>---------</h2>
    }
    foreach (FlowModel<int> f in ViewBag.Model.list_int)
    {
        <h2>@f.name = @f.value</h2>
        <h2>---------</h2>
    }
}
</div>